{
  "name": "nodejs-mongo-api-boilerplate",
  "version": "1.0.0",
  "description": "A NodeJS-Mongo-Express API boilerplate with Geospatial in Mind",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "engines": {
    "node": "8.9.4"
  },
  "repository": "https://github.com/erick-otenyo/nodejs-mongo-api-boilerplate.git",
  "author": "Erick Otenyo",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "body-parser": "1.15.0",
    "cors": "2.7.1",
    "dotenv": "^5.0.1",
    "errorhandler": "1.4.3",
    "eslint": "^3.1.1",
    "express": "4.13.4",
    "express-jwt": "3.3.0",
    "express-session": "1.13.0",
    "helmet": "^3.10.0",
    "jsonwebtoken": "7.1.9",
    "method-override": "2.3.5",
    "methods": "1.1.2",
    "mongoose": "^5.0.2",
    "mongoose-plugin-autoinc": "^1.1.6",
    "mongoose-unique-validator": "^2.0.0",
    "morgan": "1.7.0",
    "nodemon": "^1.9.2",
    "passport": "0.3.2",
    "passport-local": "1.0.0",
    "request": "2.69.0"
  },
  "bugs": {
    "url": "https://github.com/erick-otenyo/nodejs-mongo-api-boilerplate/issues"
  },
  "homepage": "https://github.com/erick-otenyo/nodejs-mongo-api-boilerplate/#readme",
  "private": false,
  "devDependencies": {}
}
